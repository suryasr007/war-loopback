{
  "name": "projects",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "tags": {
      "type": "string"
    },
    "description": {
      "type": "string",
      "dataType": "longtext"
    },
    "image": {
      "type": "string",
      "dataType": "longtext"
    },
    "updated": {
      "type": "date",
      "required": true,
      "dataType": "timestamp"
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "id",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
